<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mhy.commodity.mapper.ClassifyMapper">

    <select id="queryAllClassifies" resultType="classify">
        select classify_id, name from t_commodity_classify
    </select>

    <select id="queryAllCounts" resultType="int">
        select count(*) from t_commodity_classify
    </select>

</mapper>